			GameInstance.GetStatsSystem( GetGame() ).UnregisterListener( GetEntityID(), m_allStatsListener );
			GameInstance.GetTransactionSystem( GetGame() ).UnregisterAttachmentSlotListener( this, m_rightHandAttachmentSlotListener );
		}
	}

