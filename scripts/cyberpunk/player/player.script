	private function RemoveHandlingModifiersFromWeapon()
	{
		var weaponId : EntityID;
		var statsSystem : StatsSystem;
		var i, limit : Int32;
		weaponId = m_equippedRightHandWeapon.GetEntityID();
		statsSystem = GameInstance.GetStatsSystem( GetGame() );
		for( i = 0, limit = m_handlingModifiers.recoilGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.RemoveModifier( weaponId, m_handlingModifiers.recoilGroup[ i ] );
		}
		for( i = 0, limit = m_handlingModifiers.timeOutGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.RemoveModifier( weaponId, m_handlingModifiers.timeOutGroup[ i ] );
		}
		for( i = 0, limit = m_handlingModifiers.multSwayGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.RemoveModifier( weaponId, m_handlingModifiers.multSwayGroup[ i ] );
		}
		for( i = 0, limit = m_handlingModifiers.addSwayGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.RemoveModifier( weaponId, m_handlingModifiers.addSwayGroup[ i ] );
		}
		for( i = 0, limit = m_handlingModifiers.spreadGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.RemoveModifier( weaponId, m_handlingModifiers.spreadGroup[ i ] );
		}
	}

