		m_handlingModifiers.multSwayGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SwaySideMaximumAngleDistance, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		m_handlingModifiers.multSwayGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SwaySideMinimumAngleDistance, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		for( i = 0, limit = m_handlingModifiers.multSwayGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.AddModifier( GetEntityID(), m_handlingModifiers.multSwayGroup[ i ] );
		}
		m_handlingModifiers.addSwayGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SwaySideTopAngleLimit, gameStatModifierType.Additive, 1.0 ) ) ) );
		m_handlingModifiers.addSwayGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SwaySideBottomAngleLimit, gameStatModifierType.Additive, 1.0 ) ) ) );
		for( i = 0, limit = m_handlingModifiers.addSwayGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.AddModifier( GetEntityID(), m_handlingModifiers.addSwayGroup[ i ] );
		}
		m_handlingModifiers.spreadGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SpreadDefaultX, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		m_handlingModifiers.spreadGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SpreadAdsDefaultX, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		m_handlingModifiers.spreadGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SpreadDefaultY, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		m_handlingModifiers.spreadGroup.PushBack( ( ( gameConstantStatModifierData )( RPGManager.CreateStatModifier( gamedataStatType.SpreadAdsDefaultY, gameStatModifierType.Multiplier, 1.0 ) ) ) );
		for( i = 0, limit = m_handlingModifiers.spreadGroup.Size(); i < limit; i += 1 )
		{
			statsSystem.AddModifier( GetEntityID(), m_handlingModifiers.spreadGroup[ i ] );
		}
	}

