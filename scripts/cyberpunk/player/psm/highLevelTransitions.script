		if( sceneTier )
		{
			requestedSceneTier = sceneTier.tier;
		}
		return ( ( GameplayTier )( requestedSceneTier ) );
	}

	protected const virtual function SceneTierToEnter() : GameplayTier
	{
		return GameplayTier.Undefined;
	}

}

