		{
			if( stateContext.GetBoolParameter( 'enteredWaterFromSceneTierII', true ) || stateContext.IsStateMachineActive( 'LocomotionTakedown' ) )
			{
				return false;
			}
			return true;
		}
		return false;
	}

}

