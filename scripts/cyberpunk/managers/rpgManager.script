		{
			return false;
		}
		if( !( TS.HasItem( player, itemID ) ) )
		{
			TS.GiveItem( player, itemID, 1 );
		}
		TS.RemoveItemFromSlot( player, T"AttachmentSlots.PersonalLink" );
		TS.AddItemToSlot( player, T"AttachmentSlots.PersonalLink", itemID );
