	}

	public static function ItemQualityNameToValue( q : CName ) : Float
	{
		var val : Float;
		switch( q )
		{
			case 'Common':
				val = 0.0;
			break;
			case 'Uncommon':
				val = 1.0;
			break;
			case 'Rare':
				val = 2.0;
			break;
			case 'Epic':
				val = 3.0;
			break;
			case 'Legendary':
				val = 4.0;
			break;
			default:
				val = 0.0;
			break;
		}
		return val;
	}

