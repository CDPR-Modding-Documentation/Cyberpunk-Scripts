	public static function ApplyAbilityGroup( owner : weak< GameObject >, group : weak< GameplayAbilityGroup_Record > )
	{
		var abilities : array< weak< GameplayAbility_Record > >;
		if( group.GetAbilitiesCount() > 0 )
		{
