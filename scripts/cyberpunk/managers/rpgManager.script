	public static function IsRicochetChanceEnabled( gi : GameInstance, owner : GameObject, itemID : ItemID ) : Bool
	{
		var itemData : gameItemData;
		itemData = GameInstance.GetTransactionSystem( gi ).GetItemData( owner, itemID );
		if( itemData )
