			SetFactValue( gi, 'prologue_sts_done', factVal + 1 );
			SetFactValue( gi, 'stsExperienceSum', GetFact( gi, 'stsExperienceSum' ) + ( ( Int32 )( experienceReward ) ) );
		}
		else
		{
			stsCount = GetFact( gi, 'prologue_sts_done' );
			if( ( stsAmount - stsCount ) > 0 )
			{
