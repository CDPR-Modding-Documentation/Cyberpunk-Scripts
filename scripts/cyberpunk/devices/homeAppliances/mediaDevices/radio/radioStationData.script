	public static function GetStationNameByIndex( index : Int32, optional isUI : Bool ) : CName
	{
		if( index == -1 )
		{
			return 'station_none';
		}
		return GetStationName( ( ( isUI ) ? ( GetRadioStationByUIIndex( index ) ) : ( ( ( ERadioStationList )( index ) ) ) ) );
	}

