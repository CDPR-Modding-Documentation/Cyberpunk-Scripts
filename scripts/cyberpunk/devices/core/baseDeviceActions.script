	public function IsEyesInTheSkyPerk() : Bool
	{
		var hackCategory : gamedataHackCategory;
		var playerPuppet : PlayerPuppet;
		var playerBlackboard : IBlackboard;
		var isControlling : Bool;
		var isCategory : Bool;
		var isPerk : Bool;
		isControlling = false;
		isCategory = false;
		isPerk = false;
		playerPuppet = ( ( PlayerPuppet )( GetPlayer( GetExecutor().GetGame() ) ) );
		if( !( playerPuppet ) )
		{
			return false;
		}
		hackCategory = GetObjectActionRecord().HackCategory().Type();
		playerBlackboard = GameInstance.GetBlackboardSystem( GetExecutor().GetGame() ).GetLocalInstanced( playerPuppet.GetEntityID(), GetAllBlackboardDefs().PlayerStateMachine );
		isCategory = ( hackCategory == gamedataHackCategory.CovertHack || hackCategory == gamedataHackCategory.ControlHack ) || hackCategory == gamedataHackCategory.DeviceHack;
		isControlling = playerBlackboard.GetBool( GetAllBlackboardDefs().PlayerStateMachine.IsControllingDevice );
		isPerk = PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Left_Milestone_1 );
		return ( isCategory && isControlling ) && isPerk;
	}

	public function GetModifiedDurationTime( baseValue : Float ) : Float
	{
		if( IsEyesInTheSkyPerk() )
		{
			return baseValue + ( baseValue * TDB.GetFloat( T"NewPerks.Intelligence_Left_Milestone_1.durationIncease", 0.0 ) );
		}
		return baseValue;
	}

	public virtual function GetDurationTime() : Float
	{
		var timeMods : array< weak< StatModifier_Record > >;
		var durationTime : Float;
		var executor : GameObject;
		if( GetObjectActionRecord() )
		{
			GetObjectActionRecord().DurationTime( timeMods );
		}
		executor = GetExecutor();
		if( executor && ( timeMods.Size() > 0 ) )
		{
			durationTime = RPGManager.CalculateStatModifiers( timeMods, executor.GetGame(), executor, GetRequesterID(), executor.GetEntityID() );
		}
		return GetModifiedDurationTime( durationTime );
	}

	public function CanPayCost( optional user : GameObject, optional checkForOverclockedState : Bool ) : Bool
	{
		var statPoolSys : StatPoolsSystem;
		var transactionSys : TransactionSystem;
		var i : Int32;
		var statPoolCost : weak< StatPoolCost_Record >;
		var itemCost : weak< ItemCost_Record >;
		var quantity : Int32;
		var statPoolType : gamedataStatPoolType;
		var executorQuantity : Int32;
		var executor : GameObject;
		var overclockIsActivated : Bool;
		if( user )
		{
			executor = user;
		}
		else
		{
			executor = GetExecutor();
		}
		if( m_costComponents.Size() == 0 )
		{
			GetObjectActionRecord().Costs( m_costComponents );
		}
		for( i = 0; i < m_costComponents.Size(); i += 1 )
		{
			quantity = GetCost();
			itemCost = ( ( ItemCost_Record )( m_costComponents[ i ] ) );
			statPoolCost = ( ( StatPoolCost_Record )( m_costComponents[ i ] ) );
			if( itemCost )
			{
				transactionSys = GameInstance.GetTransactionSystem( executor.GetGame() );
				executorQuantity = transactionSys.GetItemQuantity( executor, ItemID.CreateQuery( itemCost.Item().GetID() ) );
			}
			else if( statPoolCost )
			{
				statPoolSys = GameInstance.GetStatPoolsSystem( executor.GetGame() );
				statPoolType = statPoolCost.StatPool().StatPoolType();
				executorQuantity = FloorF( statPoolSys.GetStatPoolValue( executor.GetEntityID(), statPoolType, false ) );
			}
			if( executorQuantity < quantity )
			{
				overclockIsActivated = QuickHackableHelper.IsOverclockedStateActive( GetExecutor() );
				if( checkForOverclockedState && overclockIsActivated )
				{
					if( !( QuickHackableHelper.CanPayWithHealthInOverclockedState( GetExecutor(), quantity, 0.0 ) ) )
					{
						return false;
					}
				}
				else
				{
					return false;
				}
			}
		}
		return true;
	}

	public function CanSkipPayCost( isJustConsulting : Bool ) : Bool
	{
		var playerPuppet : PlayerPuppet;
		var playerBlackboard : IBlackboard;
		var playerFreeCostActionIDName : TweakDBID;
		var amountOfFreeSimultaneousActions : Int32;
		if( m_canSkipPayCost )
		{
			return true;
		}
		playerPuppet = ( ( PlayerPuppet )( GetExecutor() ) );
		if( !( playerPuppet ) )
		{
			return false;
		}
		playerBlackboard = GameInstance.GetBlackboardSystem( playerPuppet.GetGame() ).GetLocalInstanced( playerPuppet.GetEntityID(), GetAllBlackboardDefs().PlayerStateMachine );
		playerFreeCostActionIDName = ( ( TweakDBID )( playerBlackboard.GetVariant( GetAllBlackboardDefs().PlayerStateMachine.CostFreeActionID ) ) );
		amountOfFreeSimultaneousActions = playerBlackboard.GetInt( GetAllBlackboardDefs().PlayerStateMachine.AmountOfCostFreeActions );
		if( GetObjectActionID() == playerFreeCostActionIDName )
		{
			if( amountOfFreeSimultaneousActions > 0 )
			{
				amountOfFreeSimultaneousActions -= 1;
				playerBlackboard.SetInt( GetAllBlackboardDefs().PlayerStateMachine.AmountOfCostFreeActions, amountOfFreeSimultaneousActions );
				if( amountOfFreeSimultaneousActions > 0 )
				{
					return true;
				}
			}
			if( !( isJustConsulting ) )
			{
				playerBlackboard.SetVariant( GetAllBlackboardDefs().PlayerStateMachine.CostFreeActionID, TDBID.None() );
			}
			return true;
		}
		return false;
	}

	public virtual function PayCost( optional checkForOverclockedState : Bool ) : Bool
	{
		var statPoolSys : StatPoolsSystem;
		var transactionSys : TransactionSystem;
		var currValue : Float;
		var newValue : Float;
		var statPoolCost : weak< StatPoolCost_Record >;
		var itemCost : weak< ItemCost_Record >;
		var quantity : Int32;
		var statPoolType : gamedataStatPoolType;
		var executorQuantity : Int32;
		var overclockIsActivated : Bool;
		if( GetObjectActionRecord() )
		{
			if( m_costComponents.Size() == 0 )
			{
				GetObjectActionRecord().Costs( m_costComponents );
			}
		}
		if( m_costComponents[ 0 ] )
		{
			quantity = GetCost();
			this.paymentQuantity = quantity;
			itemCost = ( ( ItemCost_Record )( m_costComponents[ 0 ] ) );
			statPoolCost = ( ( StatPoolCost_Record )( m_costComponents[ 0 ] ) );
			overclockIsActivated = QuickHackableHelper.IsOverclockedStateActive( GetExecutor() );
			if( itemCost )
			{
				transactionSys = GameInstance.GetTransactionSystem( GetExecutor().GetGame() );
				executorQuantity = transactionSys.GetItemQuantity( GetExecutor(), ItemID.CreateQuery( itemCost.Item().GetID() ) );
				if( executorQuantity < quantity )
				{
					if( checkForOverclockedState && overclockIsActivated )
					{
						return QuickHackableHelper.PayWithHealthInOverclockedState( GetExecutor(), quantity );
					}
					else
					{
						return false;
					}
				}
				transactionSys.RemoveItem( GetExecutor(), ItemID.CreateQuery( itemCost.Item().GetID() ), quantity );
			}
			else if( statPoolCost )
			{
				statPoolSys = GameInstance.GetStatPoolsSystem( GetExecutor().GetGame() );
				statPoolType = statPoolCost.StatPool().StatPoolType();
				currValue = statPoolSys.GetStatPoolValue( GetExecutor().GetEntityID(), statPoolType, false );
				newValue = currValue - ( ( Float )( quantity ) );
				if( newValue < 0.0 )
				{
					if( checkForOverclockedState && overclockIsActivated )
					{
						return QuickHackableHelper.PayWithHealthInOverclockedState( GetExecutor(), quantity );
					}
					else
					{
						return false;
					}
				}
				statPoolSys.RequestSettingStatPoolValue( GetExecutor().GetEntityID(), statPoolType, newValue, GetExecutor(), false );
			}
			GameObject.PlaySoundEvent( GetExecutor(), 'ui_focus_mode_quickhack' );
			return true;
		}
		return true;
	}

	public virtual function GetCost() : Int32
	{
		var costMods : array< weak< StatModifier_Record > >;
		var targetID : EntityID;
		var targetPuppet : ScriptedPuppet;
		var statPoolCost : StatPoolCost_Record;
		var cost, extraCost : Float;
		var availableMemory : Float;
		var stacks : Uint32;
		var device : ScriptableDeviceComponentPS;
		var hackCategory : gamedataHackCategory;
		var statsDataSystem : StatsDataSystem;
		var currentlyUploadingAction : weak< ScriptableDeviceAction >;
		var deviceActionQueue : weak< DeviceActionQueue >;
		var objectActionRecords : array< ObjectAction_Record >;
		var targetPos : Vector4;
		var instigatorPos : Vector4;
		var distance : Float;
		var hackID : TweakDBID;
		var shouldReduceCost : Bool;
		var i : Int32;
		hackID = GetObjectActionRecord().GetID();
		if( GetExecutor() && ( GetObjectActionRecord().GetCostsCount() > 0 ) )
		{
			device = ( ( ScriptableDeviceComponentPS )( GetOwnerPS( GetExecutor().GetGame() ) ) );
			if( ( device && ( GetObjectActionID() == T"DeviceAction.TakeControlCameraClassHack" ) ) && device.WasActionPerformed( GetActionID(), EActionContext.QHack ) )
			{
				return 0;
			}
			else
			{
				if( m_costComponents.Size() == 0 )
				{
					GetObjectActionRecord().Costs( m_costComponents );
				}
				if( m_costComponents[ 0 ] )
				{
					GetCostMods( m_costComponents, costMods );
					if( EntityID.IsDefined( GetRequesterID() ) )
					{
						targetID = GetRequesterID();
					}
					else
					{
						targetID = PersistentID.ExtractEntityID( GetPersistentID() );
					}
					cost = RPGManager.CalculateStatModifiers( costMods, GetExecutor().GetGame(), GetExecutor(), targetID, GetExecutor().GetEntityID() );
					statPoolCost = ( ( StatPoolCost_Record )( m_costComponents[ 0 ] ) );
					if( statPoolCost.StatPool().StatPoolType() == gamedataStatPoolType.Memory )
					{
						hackCategory = GetObjectActionRecord().HackCategory().Type();
						if( hackCategory == gamedataHackCategory.VehicleHack )
						{
							if( GetObjectActionID() == T"DeviceAction.TakeControlVehicleClassHack" )
							{
								cost *= GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetExecutorLevel(), 'vehicle_quickhack_remotecontrol_memory_cost_multiplier' );
							}
							else if( GetObjectActionID() == T"DeviceAction.VehicleForceBrakesClassHack" )
							{
								cost *= GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetExecutorLevel(), 'vehicle_quickhack_forcebrakes_memory_cost_multiplier' );
							}
							else if( GetObjectActionID() == T"DeviceAction.VehicleExplodeClassHack" )
							{
								cost *= GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetExecutorLevel(), 'vehicle_quickhack_explode_memory_cost_multiplier' );
							}
							else if( GetObjectActionID() == T"DeviceAction.VehicleAccelerateClassHack" )
							{
								cost *= GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetExecutorLevel(), 'vehicle_quickhack_accelerate_memory_cost_multiplier' );
							}
						}
						else
						{
							extraCost = GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetPowerLevelDiff(), 'pl_diff_to_memory_cost_modifier' );
							cost += extraCost;
							if( hackCategory == gamedataHackCategory.UltimateHack )
							{
								cost += extraCost;
							}
						}
						if( PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Left_Perk_3_1 ) && IsFirstUniqueCategoryInQueue( targetID, hackCategory ) )
						{
							cost -= GameInstance.GetStatsSystem( GetExecutor().GetGame() ).GetStatValue( GetExecutor().GetEntityID(), gamedataStatType.FirstHackOfTypeInQueueRAMDecrease );
						}
						if( hackCategory == gamedataHackCategory.DamageHack && PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Central_Perk_2_2 ) )
						{
							shouldReduceCost = false;
							if( ( GameInstance.GetStatusEffectSystem( GetExecutor().GetGame() ).HasStatusEffectWithTag( targetID, 'CovertQuickhacked' ) || GameInstance.GetStatusEffectSystem( GetExecutor().GetGame() ).HasStatusEffectWithTag( targetID, 'ControlQuickhacked' ) ) || GameInstance.GetStatusEffectSystem( GetExecutor().GetGame() ).HasStatusEffect( targetID, T"BaseStatusEffect.DistractionDuration" ) )
							{
								shouldReduceCost = true;
							}
							if( !( shouldReduceCost ) )
							{
								targetPuppet = ( ( ScriptedPuppet )( GameInstance.FindEntityByID( GetExecutor().GetGame(), targetID ) ) );
								if( targetPuppet )
								{
									currentlyUploadingAction = targetPuppet.GetCurrentlyUploadingAction();
									if( currentlyUploadingAction.GetObjectActionRecord().HackCategory().Type() == gamedataHackCategory.CovertHack || currentlyUploadingAction.GetObjectActionRecord().HackCategory().Type() == gamedataHackCategory.ControlHack )
									{
										shouldReduceCost = true;
									}
									deviceActionQueue = currentlyUploadingAction.GetDeviceActionQueue();
									deviceActionQueue.GetAllQueuedActionObjectRecords( objectActionRecords );
									for( i = 0; i < objectActionRecords.Size(); i += 1 )
									{
										if( objectActionRecords[ i ].HackCategory().Type() == gamedataHackCategory.CovertHack || objectActionRecords[ i ].HackCategory().Type() == gamedataHackCategory.ControlHack )
										{
											shouldReduceCost = true;
											break;
										}
									}
								}
							}
							if( shouldReduceCost )
							{
								cost -= TDB.GetFloat( T"NewPerks.Intelligence_Central_Perk_2_2.memoryCostReduction", 0.0 );
							}
						}
						if( IsEyesInTheSkyPerk() )
						{
							cost -= TDB.GetFloat( T"NewPerks.Intelligence_Left_Milestone_1.memoryCostReduction", 0.0 );
						}
						if( PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Master_Perk_1 ) )
						{
							targetPuppet = ( ( ScriptedPuppet )( GameInstance.FindEntityByID( GetExecutor().GetGame(), targetID ) ) );
							if( ( targetPuppet && targetPuppet.CanNewActionBeQueued() ) && ( targetPuppet.GetDeviceActionQueueSize() >= ( targetPuppet.GetDeviceActionMaxQueueSize() - 1 ) ) )
							{
								cost *= ( 1.0 - TDB.GetFloat( T"NewPerks.Intelligence_Master_Perk_1.memoryCostReduction", 0.0 ) );
							}
						}
						if( ( GameInstance.GetStatPoolsSystem( GetExecutor().GetGame() ).GetStatPoolValue( GetExecutor().GetEntityID(), gamedataStatPoolType.QuickHackUpload, true ) > 0.0 ) && ( GameInstance.GetStatsSystem( GetExecutor().GetGame() ).GetStatValue( targetID, gamedataStatType.IsNetrunnerArchetype ) > 0.0 ) )
						{
							if( GameInstance.GetStatsSystem( GetExecutor().GetGame() ).GetStatValue( targetID, gamedataStatType.RevealNetrunnerWhenHacked ) > 0.0 )
							{
								if( PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Left_Perk_2_1 ) )
								{
									cost -= TDB.GetFloat( T"NewPerks.Intelligence_Left_Perk_2_1.memoryCostReduction", 0.0 );
								}
							}
						}
						if( ( ( hackID == T"QuickHack.MadnessLvl3Hack" ) || ( hackID == T"QuickHack.MadnessLvl4Hack" ) ) || ( hackID == T"QuickHack.MadnessSetFriendlyHack" ) )
						{
							cost -= GetMadnessLvl3ProgramCostReduction( targetID );
						}
						if( ( hackID == T"QuickHack.BrainMeltLvl3Hack" ) || ( hackID == T"QuickHack.BrainMeltLvl4Hack" ) )
						{
							stacks = StatusEffectHelper.GetStatusEffectByID( GetPlayer( GetExecutor().GetGame() ), T"BaseStatusEffect.BrainMeltCostReductionSE" ).GetStackCount();
							cost -= ( ( ( Float )( stacks ) ) * TDB.GetFloat( T"EquipmentGLP.BrainMeltProgramLvl3Passive.memoryCostReductionPerStack", 0.0 ) );
						}
						if( ( hackID == T"QuickHack.SystemCollapseLvl3Hack" ) || ( hackID == T"QuickHack.SystemCollapseLvl4Hack" ) )
						{
							stacks = StatusEffectHelper.GetStatusEffectByID( GetPlayer( GetExecutor().GetGame() ), T"BaseStatusEffect.SystemCollapseMemoryCostReduction" ).GetStackCount();
							cost -= ( ( ( Float )( stacks ) ) * TDB.GetFloat( T"EquipmentGLP.SystemCollapseLvl3Program.memoryCostReductionPerStack", 0.0 ) );
						}
						if( ( GetObjectActionRecord().GetID() == T"QuickHack.GrenadeLvl3Hack" ) || ( GetObjectActionRecord().GetID() == T"QuickHack.GrenadeLvl4Hack" ) )
						{
							cost -= GetDetonateGranadeCostReduction( false );
						}
						else if( GetObjectActionRecord().GetID() == T"QuickHack.GrenadeLvl4PlusPlusHack" )
						{
							cost -= GetDetonateGranadeCostReduction( true );
						}
						if( actionName == 'Suicide' )
						{
							cost -= GameInstance.GetStatsSystem( GetExecutor().GetGame() ).GetStatValue( GetExecutor().GetEntityID(), gamedataStatType.SuicideHackMemoryCostReduction );
						}
						if( PlayerDevelopmentSystem.GetData( GetExecutor() ).IsNewPerkBought( gamedataNewPerkType.Intelligence_Central_Perk_1_2 ) )
						{
							targetPos = GameInstance.FindEntityByID( GetExecutor().GetGame(), targetID ).GetWorldPosition();
							instigatorPos = GetExecutor().GetWorldPosition();
							distance = Vector4.Distance( targetPos, instigatorPos );
							statsDataSystem = GameInstance.GetStatsDataSystem( GetExecutor().GetGame() );
							cost *= ( 1.0 - statsDataSystem.GetValueFromCurve( 'hacking_passives', distance, 'distance_to_hacking_cost_reduction' ) );
						}
						if( QuickHackableHelper.IsOverclockedStateActive( GetExecutor() ) )
						{
							if( StrEndsWith( NameToString( actionName ), "BlackWall" ) )
							{
								availableMemory = GameInstance.GetStatPoolsSystem( GetExecutor().GetGame() ).GetStatPoolValue( GetExecutor().GetEntityID(), gamedataStatPoolType.Memory, false );
								availableMemory = MinF( cost, ( ( Float )( FloorF( availableMemory ) ) ) );
								cost = availableMemory + ( ( cost - availableMemory ) * TDB.GetFloat( T"QuickHack.BaseBlackWallHack.memoryCostReductionInOverclock", 1.0 ) );
							}
						}
					}
					if( costMods.Size() > 0 )
					{
						return Max( 2, CeilF( cost ) );
					}
					return Max( 0, CeilF( cost ) );
				}
			}
		}
		return 0;
	}

	public virtual function GetBaseCost() : Int32
	{
		var costMods : array< weak< StatModifier_Record > >;
		var targetID : EntityID;
		var statPoolCost : weak< StatPoolCost_Record >;
		var cost, extraCost : Float;
		var constantCostMods : array< weak< StatModifier_Record > >;
		var i : Int32;
		if( GetExecutor() && ( GetObjectActionRecord().GetCostsCount() > 0 ) )
		{
			if( m_costComponents.Size() == 0 )
			{
				GetObjectActionRecord().Costs( m_costComponents );
			}
			GetCostMods( m_costComponents, costMods );
			if( m_costComponents[ 0 ] )
			{
				if( EntityID.IsDefined( GetRequesterID() ) )
				{
					targetID = GetRequesterID();
				}
				else
				{
					targetID = PersistentID.ExtractEntityID( GetPersistentID() );
				}
				for( i = 0; i < costMods.Size(); i += 1 )
				{
					if( ( ( ConstantStatModifier_Record )( costMods[ i ] ) ) )
					{
						constantCostMods.PushBack( costMods[ i ] );
					}
				}
				cost = RPGManager.CalculateStatModifiers( constantCostMods, GetExecutor().GetGame(), GetExecutor(), targetID, GetExecutor().GetEntityID() );
				statPoolCost = ( ( StatPoolCost_Record )( m_costComponents[ 0 ] ) );
				if( statPoolCost.StatPool().StatPoolType() == gamedataStatPoolType.Memory )
				{
					extraCost = GameInstance.GetStatsDataSystem( GetExecutor().GetGame() ).GetValueFromCurve( 'puppet_dynamic_scaling', GetPowerLevelDiff(), 'pl_diff_to_memory_cost_modifier' );
					cost += extraCost;
					if( GetObjectActionRecord().HackCategory().Type() == gamedataHackCategory.UltimateHack )
					{
						cost += extraCost;
					}
				}
				if( costMods.Size() > 0 )
				{
					return Max( 1, CeilF( cost ) );
				}
				return Max( 0, CeilF( cost ) );
			}
		}
		return 0;
	}

	public static function GetBaseCostStatic( executor : weak< GameObject >, actionRecord : weak< ObjectAction_Record > ) : Int32
	{
		var costComponents : array< weak< ObjectActionCost_Record > >;
		var costMods : array< weak< StatModifier_Record > >;
		var targetID : EntityID;
		var cost : Float;
		var constantCostMods : array< weak< StatModifier_Record > >;
		var i : Int32;
		if( executor && ( actionRecord.GetCostsCount() > 0 ) )
		{
			actionRecord.Costs( costComponents );
			GetCostMods( costComponents, costMods );
			if( costComponents[ 0 ] )
			{
				for( i = 0; i < costMods.Size(); i += 1 )
				{
					if( ( ( ConstantStatModifier_Record )( costMods[ i ] ) ) )
					{
						constantCostMods.PushBack( costMods[ i ] );
					}
				}
				cost += RPGManager.CalculateStatModifiers( constantCostMods, executor.GetGame(), executor, targetID, executor.GetEntityID() );
				if( costMods.Size() > 0 )
				{
					return Max( 1, CeilF( cost ) );
				}
				return Max( 0, CeilF( cost ) );
			}
		}
		return 0;
	}

	public static function GetCostMods( costComponents : ref< array< weak< ObjectActionCost_Record > > >, out costMods : array< weak< StatModifier_Record > > )
	{
		var i : Int32;
		for( i = 0; i < costComponents.Size(); i += 1 )
		{
			costComponents[ i ].CostMods( costMods );
		}
	}

	private function GetPowerLevelDiff() : Float
	{
		var statsSystem : StatsSystem;
		var powerLevelDiff : Float;
		var executorLevel : Float;
		var targetLevel : Float;
		var targetID : EntityID;
		if( !( GetExecutor() ) )
		{
			return 0.0;
		}
		targetID = GetRequesterID();
		if( !( EntityID.IsDefined( targetID ) ) )
		{
			targetID = PersistentID.ExtractEntityID( GetPersistentID() );
		}
		if( !( EntityID.IsDefined( targetID ) ) )
		{
			return 0.0;
		}
		statsSystem = GameInstance.GetStatsSystem( GetExecutor().GetGame() );
		executorLevel = statsSystem.GetStatValue( GetExecutor().GetEntityID(), gamedataStatType.PowerLevel );
		targetLevel = statsSystem.GetStatValue( targetID, gamedataStatType.PowerLevel );
		powerLevelDiff = ( ( Float )( RoundMath( executorLevel ) - RoundF( targetLevel ) ) );
		return powerLevelDiff;
	}

	private function GetExecutorLevel() : Float
	{
		var statsSystem : StatsSystem;
		var executorLevel : Float;
		if( !( GetExecutor() ) )
		{
			return 0.0;
		}
		statsSystem = GameInstance.GetStatsSystem( GetExecutor().GetGame() );
		executorLevel = statsSystem.GetStatValue( GetExecutor().GetEntityID(), gamedataStatType.PowerLevel );
		return executorLevel;
	}

	private function GetMadnessLvl3ProgramCostReduction( targetID : EntityID ) : Float
	{
		var maxReduction : Float;
		var stackReduction : Float;
		var costReductors : array< TweakDBID >;
		var costReductor : gamedataStatusEffectType;
		var appliedStatusEffects : array< StatusEffect >;
		var stacks : Uint32;
		var i, j : Int32;
		maxReduction = TweakDBInterface.GetFloat( T"Items.MadnessLvl3Program.maxCostReduction", 0.0 );
		stackReduction = TweakDBInterface.GetFloat( T"Items.MadnessLvl3Program.costReductionPerReductorStack", 0.0 );
		costReductors = TweakDBInterface.GetForeignKeyArray( T"Items.MadnessLvl3Program.statusEffectsTypeCostReductors" );
		stacks = 0;
		for( i = 0; i < costReductors.Size(); i += 1 )
		{
			appliedStatusEffects.Clear();
			costReductor = TweakDBInterface.GetStatusEffectTypeRecord( costReductors[ i ] ).Type();
			GameInstance.GetStatusEffectSystem( GetExecutor().GetGame() ).GetAppliedEffectsOfType( targetID, costReductor, appliedStatusEffects );
			for( j = 0; j < appliedStatusEffects.Size(); j += 1 )
			{
				stacks += appliedStatusEffects[ j ].GetStackCount();
			}
		}
		return MinF( maxReduction, ( ( Float )( stacks ) ) * stackReduction );
	}

	private function GetDetonateGranadeCostReduction( legendaryPlusPlus : Bool ) : Float
	{
		var stackSE : TweakDBID;
		var stackReduction : Float;
		var stacks : Uint32;
		stackSE = ( ( legendaryPlusPlus ) ? ( T"BaseStatusEffect.ReduceUltimateSuicideWithGrenadeCostPlusPlus" ) : ( T"BaseStatusEffect.ReduceUltimateSuicideWithGrenadeCost" ) );
		stackReduction = TweakDBInterface.GetFloat( ( ( legendaryPlusPlus ) ? ( T"BaseStatusEffect.ReduceUltimateSuicideWithGrenadeCostPlusPlus.costReductionPerStack" ) : ( T"BaseStatusEffect.ReduceUltimateSuicideWithGrenadeCost.costReductionPerStack" ) ), 0.0 );
		stacks = StatusEffectHelper.GetStatusEffectByID( GetPlayer( GetExecutor().GetGame() ), stackSE ).GetStackCount();
		return ( ( Float )( stacks ) ) * stackReduction;
	}

}

abstract class ScriptableDeviceAction extends BaseScriptableAction
{
	var prop : DeviceActionProperty;
	protected var m_actionWidgetPackage : SActionWidgetPackage;
	protected var m_spiderbotActionLocationOverride : NodeRef;
	protected var m_duration : Float;
	private var m_canTriggerStim : Bool;
	default m_canTriggerStim = true;
	private var m_wasPerformedOnOwner : Bool;
	private var m_shouldActivateDevice : Bool;
	private var m_disableSpread : Bool;
	protected var m_isQuickHack : Bool;
	protected var m_isSpiderbotAction : Bool;
	protected var m_attachedProgram : TweakDBID;
	protected var m_activeStatusEffect : TweakDBID;
	protected var m_interactionIconType : TweakDBID;
	protected var m_hasInteraction : Bool;
	protected var m_inactiveReason : String;
	protected var m_widgetStyle : gamedataComputerUIStyle;

	protected override function GetOwnerPS( game : GameInstance ) : ScriptableDeviceComponentPS
	{
		var psID : PersistentID;
		psID = GetPersistentID();
		if( PersistentID.IsDefined( psID ) )
		{
			return ( ( ScriptableDeviceComponentPS )( GameInstance.GetPersistencySystem( game ).GetConstAccessToPSObject( psID, GetDeviceClassName() ) ) );
		}
		else
		{
			return NULL;
		}
	}

	public virtual function ResolveAction( data : ResolveActionData ) : Bool
