			if( ( ( SecuritySystemControllerPS )( ancestors[ i ] ) ) )
			{
				secSys = ( ( SecuritySystemControllerPS )( ancestors[ i ] ) );
				if( secSys && !( secSys.IsDisabled() ) )
				{
					return secSys;
				}
			}
		}
		return NULL;
	}

