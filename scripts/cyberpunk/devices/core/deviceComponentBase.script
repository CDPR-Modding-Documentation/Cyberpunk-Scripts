function BasicAvailabilityTest( device : ScriptableDeviceComponentPS ) : Bool
{
	if( device.IsDisabled() )
	{
		return false;
	}
	if( device.IsUnpowered() )
	{
		return false;
	}
	if( device.IsOFF() )
	{
		return false;
	}
	return true;
}

