	private function SetupSetButton()
	{
		var set : ClothingSet;
		var btnConrtoller : weak< MenuItemController >;
		var setLabel : String;
		set = EquipmentSystem.GetActiveWardrobeSet( m_player );
		if( ( set != NULL ) && !( ClothingSet.IsEmpty( set ) ) )
		{
			btnConrtoller = ( ( MenuItemController )( inkWidgetRef.GetController( m_btnSets ) ) );
			setLabel = "#" + ( ( String )( set.setID ) );
			btnConrtoller.UpdateButton( setLabel, set.iconID );
		}
	}

	private function SetDeckData()
	{
		var itemTags : array< CName >;
		var itemRecord : weak< Item_Record >;
		var systemReplacementID : ItemID;
		var deckSlotController : weak< InventoryWeaponDisplayController >;
		var deckItemData : InventoryItemData;
		systemReplacementID = EquipmentSystem.GetData( GetPlayerControlledObject() ).GetActiveItem( gamedataEquipmentArea.SystemReplacementCW );
		itemRecord = RPGManager.GetItemRecord( systemReplacementID );
		if( itemRecord )
		{
			itemTags = itemRecord.Tags();
		}
		if( itemTags.Contains( 'Cyberdeck' ) )
		{
			inkWidgetRef.SetVisible( m_cyberdeckLinkContainer, true );
			deckSlotController = ( ( InventoryWeaponDisplayController )( inkWidgetRef.GetControllerByType( m_cyberdeckLinkItem, 'InventoryWeaponDisplayController' ) ) );
			deckItemData = m_InventoryManager.GetItemDataFromIDInLoadout( systemReplacementID );
			deckSlotController.Setup( deckItemData, gamedataEquipmentArea.SystemReplacementCW );
			deckSlotController.RegisterToCallback( 'OnRelease', this, 'OnCyberdeckClick' );
		}
		else
		{
			inkWidgetRef.SetVisible( m_cyberdeckLinkContainer, false );
		}
	}

