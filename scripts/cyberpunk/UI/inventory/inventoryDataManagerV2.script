			result.PushBack( gamedataItemType.Wea_SniperRifle );
			result.PushBack( gamedataItemType.Wea_SubmachineGun );
			result.PushBack( gamedataItemType.Wea_TwoHandedClub );
			break;
		}
		return result;
	}

