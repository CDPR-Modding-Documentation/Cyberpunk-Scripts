struct AbilityData
{
	var Empty : Bool;
	default Empty = true;
	var ID : ItemID;
	var Name : String;
	var IconPath : String;
	var CategoryName : String;
	var Description : String;
	var EquipmentArea : gamedataEquipmentArea;
	default EquipmentArea = gamedataEquipmentArea.Invalid;
	var IsEquipped : Bool;
	default IsEquipped = false;
	var GameItemData : gameItemData;
	var AssignedIndex : Int32;
	default AssignedIndex = -1;
}

