implicit cast ( hotkey : EHotkey ) : Int32
{
	return ( ( Int32 )( hotkey ) );
}

import enum EHotkey
{
	INVALID,
	DPAD_UP,
	DPAD_DOWN,
	DPAD_RIGHT,
	RB,
	LBRB,
}

enum EHotkeyRequestType
{
	Assign = 0,
	Cycle = 1,
	Restore = 2,
}

