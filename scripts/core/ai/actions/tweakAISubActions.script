			{
				continue;
			}
			if( !( CheckPath( context, ScriptExecutionContext.GetOwner( context ).GetWorldPosition(), targetsPosition[ i ], allowedOffMeshTags ) ) )
			{
				continue;
			}
			filteredIndexes.PushBack( i );
		}
