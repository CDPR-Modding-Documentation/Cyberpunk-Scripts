			}
		}
		AICoverHelper.GetCoverBlackboard( ( ( ScriptedPuppet )( ScriptExecutionContext.GetOwner( context ) ) ) ).SetBool( GetAllBlackboardDefs().AICover.firstCoverEvaluationDone, true );
		if( consideredCoverID > ( ( Uint64 )( 0 ) ) )
