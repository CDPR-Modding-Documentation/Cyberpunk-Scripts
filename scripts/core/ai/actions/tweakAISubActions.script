			{
				continue;
			}
			isWithinMaxDistance = ( ownerDistances[ i ] <= maxOwnerDistance ) && ( distancesObj[ i ] <= maxDistanceObj );
			if( isMaxDistanceSpecified && !( isWithinMaxDistance ) )
			{
				continue;
			}
			hasLoSTarget = !( Vector4.IsZero( lineOfSightTarget ) );
