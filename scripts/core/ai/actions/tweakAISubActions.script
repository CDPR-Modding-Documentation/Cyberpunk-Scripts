	public static function SetInitialCoverData( puppet : weak< ScriptedPuppet >, const coverID : Uint64 )
	{
		var coverStance : gameCoverHeight;
		var coverFeature : AnimFeature_NPCCoverStanceState;
		var shootingSpot : Bool;
