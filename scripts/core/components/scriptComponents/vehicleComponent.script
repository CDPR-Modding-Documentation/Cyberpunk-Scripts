		if( record )
		{
			if( record.TagsContains( 'IgnoreImpulses' ) )
			{
				return;
			}
		}
		knockOverBike = new KnockOverBikeEvent;
		GetVehicle().QueueEvent( knockOverBike );
	}

	public function ReactToHPChange( destruction : Float )
	{
		var currDmgLevel : Int32;
		var vehicleRecord : weak< Vehicle_Record >;
		var destroyedAppearanceName : CName;
		var damageStageTurnOffDelayEvent : VehicleDamageStageTurnOffEvent;
		currDmgLevel = m_damageLevel;
		m_damageLevel = EvaluateDamageLevel( destruction );
		if( ( ( VehicleComponentPS )( GetPS() ) ).GetIsSubmerged() )
		{
			return;
		}
		if( m_damageLevel > currDmgLevel )
		{
			if( m_damageLevel == 1 )
			{
				UpdateDamageEngineEffects();
			}
			else if( m_damageLevel == 2 )
			{
				UpdateDamageEngineEffects();
				TutorialCarDamageFact();
				damageStageTurnOffDelayEvent = new VehicleDamageStageTurnOffEvent;
				GameInstance.GetDelaySystem( GetVehicle().GetGame() ).DelayEvent( GetVehicle(), damageStageTurnOffDelayEvent, RandRangeF( 35.0, 55.0 ) );
			}
			else if( m_damageLevel == 3 )
			{
				vehicleRecord = GetVehicle().GetRecord();
				UpdateDamageEngineEffects();
				destroyedAppearanceName = vehicleRecord.DestroyedAppearance();
				BroadcastEnvironmentalHazardStimuli();
				DisableVehicle();
				if( IsNameValid( destroyedAppearanceName ) )
				{
					GetVehicle().PrefetchAppearanceChange( destroyedAppearanceName );
				}
				if( m_mountedPlayer )
				{
					GameInstance.GetStatPoolsSystem( GetVehicle().GetGame() ).RequestSettingStatPoolValueCustomLimit( GetVehicle().GetEntityID(), gamedataStatPoolType.Health, 0.0, GetVehicle() );
					m_immuneInDecay = true;
					GamepadLightScriptableSystem.TriggerVehicleExplosionWarningSiren( GetVehicle().GetGame() );
					GameInstance.GetAudioSystem( GetVehicle().GetGame() ).AddTriggerEffect( 'te_vehicle_car_disabled', 'VehicleDisabled' );
					GameInstance.GetAudioSystem( GetVehicle().GetGame() ).PlayOnEmitter( 'v_car_explosion_warning_start', GetVehicle().GetEntityID(), 'vehicle_engine_emitter' );
					GameInstance.GetAudioSystem( GetVehicle().GetGame() ).PlayOnEmitter( 'v_car_malfunction', GetVehicle().GetEntityID(), 'vehicle_engine_emitter' );
				}
				else
				{
					GetPS().SetIsDestroyed( true );
				}
			}
			m_vehicleBlackboard.SetInt( GetAllBlackboardDefs().Vehicle.DamageState, m_damageLevel );
		}
	}

	public function ForceAboutToExplodeState()
	{
		var vehicle : VehicleObject;
		var statPoolSys : StatPoolsSystem;
		var vehicleEntityID : EntityID;
		var wantedTreshold : Float;
		vehicle = GetVehicle();
		statPoolSys = GameInstance.GetStatPoolsSystem( vehicle.GetGame() );
		vehicleEntityID = vehicle.GetEntityID();
		wantedTreshold = m_healthDecayThreshold * 0.15000001;
		if( statPoolSys.GetStatPoolValue( vehicleEntityID, gamedataStatPoolType.Health ) > wantedTreshold )
		{
			statPoolSys.RequestSettingStatPoolValue( vehicleEntityID, gamedataStatPoolType.Health, wantedTreshold, NULL );
		}
	}

	private function UpdateDamageEngineEffects()
	{
		var vehicleRecord : weak< Vehicle_Record >;
		vehicleRecord = GetVehicle().GetRecord();
		BreakAllDamageStageFX();
		if( m_damageLevel == 1 )
		{
			if( ( !( vehicleRecord.UsesSecondaryHoodEmitter() ) || GetPS().GetDoorState( EVehicleDoor.hood ) == VehicleDoorState.Open ) || GetPS().GetDoorState( EVehicleDoor.hood ) == VehicleDoorState.Detached )
			{
				GameObjectEffectHelper.StartEffectEvent( GetVehicle(), 'damage_engine_stage1', false );
			}
			else
			{
				GameObjectEffectHelper.StartEffectEvent( GetVehicle(), 'damage_engine_stage1b', false );
			}
		}
		else if( m_damageLevel == 2 )
		{
			if( ( !( vehicleRecord.UsesSecondaryHoodEmitter() ) || GetPS().GetDoorState( EVehicleDoor.hood ) == VehicleDoorState.Open ) || GetPS().GetDoorState( EVehicleDoor.hood ) == VehicleDoorState.Detached )
			{
				GameObjectEffectHelper.StartEffectEvent( GetVehicle(), 'damage_engine_stage2', false );
			}
			else
			{
				GameObjectEffectHelper.StartEffectEvent( GetVehicle(), 'damage_engine_stage2b', false );
			}
		}
		else if( m_damageLevel == 3 )
		{
			GameObjectEffectHelper.StartEffectEvent( GetVehicle(), 'damage_engine_stage3', false );
		}
	}

