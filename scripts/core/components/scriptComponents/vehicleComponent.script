	private function EvaluateDamageLevel( destruction : Float ) : Int32
	{
		if( ( destruction <= 50.0 ) && ( destruction > 25.0 ) )
		{
			m_damageLevel = 1;
		}
		else if( ( destruction <= 25.0 ) && ( destruction > m_healthDecayThreshold ) )
		{
			m_damageLevel = 2;
		}
		else if( ( destruction <= m_healthDecayThreshold ) && ( destruction > 0.0 ) )
		{
			m_damageLevel = 3;
		}
		if( GameInstance.GetGodModeSystem( GetVehicle().GetGame() ).HasGodMode( GetVehicle().GetEntityID(), gameGodModeType.Immortal ) )
		{
			if( m_damageLevel > 2 )
			{
				m_damageLevel = 2;
			}
		}
		return m_damageLevel;
	}

