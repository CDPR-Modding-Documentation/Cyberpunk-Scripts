	protected function PlayVehicleVisualCustomizationShader( play : Bool, optional instant : Bool, optional blackboard : worldEffectBlackboard, optional affectRims : Bool )
	{
		var vehicle : VehicleObject;
		vehicle = GetVehicle();
		if( play )
		{
			if( instant )
			{
				GameObjectEffectHelper.StartEffectEvent( ( ( GameObject )( vehicle ) ), 'vvc_color_instant', true, blackboard, false );
				if( affectRims )
				{
					GameObjectEffectHelper.StartEffectEvent( ( ( GameObject )( vehicle ) ), 'vvc_color_rims_instant', true, blackboard, false );
				}
			}
			else
			{
				GameObjectEffectHelper.StartEffectEvent( ( ( GameObject )( vehicle ) ), 'vvc_color', true, blackboard, false );
				if( affectRims )
				{
					GameObjectEffectHelper.StartEffectEvent( ( ( GameObject )( vehicle ) ), 'vvc_color_rims', true, blackboard, false );
				}
			}
		}
		else
		{
			GameObjectEffectHelper.BreakEffectLoopEvent( vehicle, 'vvc_color' );
			GameObjectEffectHelper.BreakEffectLoopEvent( vehicle, 'vvc_color_instant' );
			GameObjectEffectHelper.BreakEffectLoopEvent( vehicle, 'vvc_color_rims' );
			GameObjectEffectHelper.BreakEffectLoopEvent( vehicle, 'vvc_color_rims_instant' );
		}
	}

