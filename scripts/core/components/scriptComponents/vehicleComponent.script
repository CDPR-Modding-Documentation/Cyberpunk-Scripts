	protected event OnVehicleFinishedMountingEvent( evt : VehicleFinishedMountingEvent )
	{
		var isDestroyed : Bool;
		var vehDataPackage : weak< VehicleDataPackage_Record >;
		var environmentalHazardStimEvent : RepeatDirectEnvironmentalHazardStimEvent;
		if( evt.isMounting )
		{
			isDestroyed = GetPS().GetIsDestroyed();
			if( !( isDestroyed ) )
			{
				if( IsDriverSlot( evt.slotID ) )
				{
					ToggleVehicleSystems( true, true, true );
				}
			}
		}
		else if( m_isBroadcastingHazardStims )
		{
			environmentalHazardStimEvent = new RepeatDirectEnvironmentalHazardStimEvent;
			environmentalHazardStimEvent.target = evt.character;
			GetVehicle().QueueEvent( environmentalHazardStimEvent );
		}
		GetVehicle().GetAIComponent().ReleaseSeat( evt.slotID );
		GetVehicleDataPackage( GetVehicle().GetGame(), GetVehicle(), vehDataPackage );
		if( vehDataPackage.DriverCombat().Type() != gamedataDriverCombatType.Doors && ( ( gamePSMVehicle )( m_mountedPlayer.GetPlayerStateMachineBlackboard().GetInt( GetAllBlackboardDefs().PlayerStateMachine.Vehicle ) ) ) != gamePSMVehicle.DriverCombat )
		{
			m_ignoreAutoDoorClose = false;
		}
	}

