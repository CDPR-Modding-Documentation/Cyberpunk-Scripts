	protected event OnVehicleStartedMountingEvent( evt : VehicleStartedMountingEvent )
	{
		var passengers : array< weak< GameObject > >;
		if( !( evt.isMounting ) )
		{
			SendVehicleStartedUnmountingEventToPS( evt.isMounting, evt.slotID, evt.character );
			GetAllPassengers( GetVehicle().GetGame(), GetVehicle().GetEntityID(), false, passengers );
			if( ( ( ( evt.slotID == 'seat_front_left' || ( passengers.Size() == 1 ) ) && !( GetVehicle().IsAbandoned() ) ) && !( GetVehicle().IsVehicleAccelerateQuickhackActive() ) ) && !( ( ( evt.animationSlotName == 'cool' || evt.animationSlotName == 'combat' ) || evt.animationSlotName == 'speed' ) ) )
			{
				ToggleVehicleSystems( false, true, true );
