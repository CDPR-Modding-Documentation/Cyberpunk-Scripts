	public function GetCurrentAppearanceColorTemplate() : VehicleVisualCustomizationTemplate
	{
		var colorTemplateRecords : weak< VehicleAppearancesToColorTemplate_Record >;
		var templateRecord : weak< VehicleColorTemplate_Record >;
		var vehicleColorTemplate : VehicleVisualCustomizationTemplate;
		var currentAppearance, currentColorVariant : CName;
		var appearanceMatch, colorVariantMatch : Bool;
		var i : Int32;
		colorTemplateRecords = GetVehicle().GetRecord().AppearancesToColorTemplate();
		if( !( colorTemplateRecords ) )
		{
			return vehicleColorTemplate;
		}
		currentAppearance = GetVehicle().GetCurrentAppearanceName();
		currentColorVariant = GetVehicle().GetCurrentColorVariantName();
		for( i = 0; i < colorTemplateRecords.GetColorTemplatesCount(); i += 1 )
		{
			templateRecord = colorTemplateRecords.GetColorTemplatesItem( i );
			if( !( templateRecord ) )
			{
				continue;
			}
			appearanceMatch = templateRecord.MatchingAppearancesContains( currentAppearance ) && currentColorVariant == '';
			colorVariantMatch = templateRecord.MatchingColorVariantContains( currentColorVariant ) && currentColorVariant != '';
			if( appearanceMatch || colorVariantMatch )
			{
				return VehicleVisualCustomizationTemplate.FromRecord( templateRecord, GetVehicle().GetRecord().TwintoneModelName() );
			}
		}
		return vehicleColorTemplate;
	}

	public function CanApplyTemplateOnVehicle( template : VehicleVisualCustomizationTemplate, optional ignoreAlreadyApplied : Bool ) : Bool
	{
		var m_vehicle : VehicleObject;
		m_vehicle = GetVehicle();
		if( !( VehicleVisualCustomizationTemplate.IsValid( template ) ) || !( m_vehicle.GetVehicleComponent().GetIsVehicleVisualCustomizationEnabled() ) )
		{
			return false;
		}
		if( m_vehicle.GetVehiclePS().GetIsVehicleVisualCustomizationBlockedByDamage() || m_vehicle.GetVehicleComponent().GetIsVehicleVisualCustomizationTeaser() )
		{
			return false;
		}
		if( !( ignoreAlreadyApplied ) && VehicleVisualCustomizationTemplate.Equals( m_vehicle.GetVehiclePS().GetVehicleVisualCustomizationTemplate(), template ) )
		{
			return false;
		}
		if( VehicleVisualCustomizationTemplate.GetType( template ) == VehicleVisualCustomizationType.Generic )
		{
			return true;
		}
		return template.uniqueData.twintoneModelName == m_vehicle.GetRecord().TwintoneModelName();
	}

	private function CheckAboutToExplodeStateOnFlip()
	{
		var speedTreshold : Float;
		var probability : Float;
		speedTreshold = TDB.GetFloat( T"vehicles.common.hostile_explode_on_flip_speed_treshold", 5.0 );
		probability = TDB.GetFloat( T"vehicles.common.hostile_explode_on_flip_probability", 0.30000001 );
