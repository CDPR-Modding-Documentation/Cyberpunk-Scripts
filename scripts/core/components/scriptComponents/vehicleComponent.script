				{
					GetVehicle().ScheduleAppearanceChange( customAppName );
				}
			}
		}
		else
		{
			if( currentAppName != defaultAppName )
			{
				if( IsNameValid( defaultAppName ) )
				{
					GetVehicle().ScheduleAppearanceChange( defaultAppName );
				}
			}
		}
	}

