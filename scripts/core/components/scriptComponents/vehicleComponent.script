		instigator = GameInstance.GetPlayerSystem( GetVehicle().GetGame() ).GetLocalPlayerControlledGameObject();
		QueueVehicleImpactLethalHitEvent( GetVehicle(), instigator, 'Explode Quickhack' );
	}

	protected event OnVehicleNotifyPassengersOfCollision( evt : VehicleNotifyPassengersOfCollision )
	{
		var i : Int32;
		var passenger : weak< GameObject >;
		var passengers : array< weak< GameObject > >;
		var gi : GameInstance;
		var vehicleID : EntityID;
		gi = GetVehicle().GetGame();
		vehicleID = GetVehicle().GetEntityID();
		GetAllPassengers( gi, vehicleID, false, passengers );
		for( i = 0; i < passengers.Size(); i += 1 )
		{
			passenger = passengers[ i ];
			if( passenger )
			{
				DamagePassengerInCollision( passenger, evt.instigator, evt.hitDirection );
			}
		}
	}

	private function DamagePassengerInCollision( passenger : weak< GameObject >, instigator : weak< GameObject >, hitDirection : Vector4 )
	{
		var hitEvent : gameHitEvent;
		var attackContext : AttackInitContext;
		var attack : IAttack;
		var player : GameObject;
		player = GetPlayer( GetVehicle().GetGame() );
		if( player == passenger )
		{
			if( !( PlayerDevelopmentSystem.GetData( player ).IsNewPerkBoughtAnyLevel( gamedataNewPerkType.Body_Right_Milestone_1 ) ) )
			{
				hitEvent = new gameHitEvent;
				hitEvent.attackData = new AttackData;
				hitEvent.target = passenger;
				attackContext.record = TweakDBInterface.GetAttackRecord( T"Attacks.PlayerCarInCollision" );
				attackContext.instigator = instigator;
				attackContext.source = instigator;
				attack = IAttack.Create( attackContext );
				hitEvent.attackData.SetAttackDefinition( attack );
				hitEvent.attackData.AddFlag( hitFlag.Nonlethal, 'Player Passenger in vehicle in collision' );
				hitEvent.attackData.AddFlag( hitFlag.CanDamageSelf, 'Player Passenger in vehicle in collision' );
				hitEvent.attackData.SetSource( instigator );
				hitEvent.attackData.SetInstigator( instigator );
				hitEvent.attackData.SetAttackDefinition( attack );
				hitEvent.hitPosition = passenger.GetWorldPosition();
				hitEvent.hitDirection = hitDirection;
				GameInstance.GetDamageSystem( GetVehicle().GetGame() ).QueueHitEvent( hitEvent, GetVehicle() );
			}
		}
		else
		{
			hitEvent = new gameHitEvent;
			hitEvent.attackData = new AttackData;
			hitEvent.target = passenger;
			attackContext.record = TweakDBInterface.GetAttackRecord( T"Attacks.NPCCarInCollision" );
			attackContext.instigator = instigator;
			attackContext.source = instigator;
			attack = IAttack.Create( attackContext );
			hitEvent.attackData.SetAttackDefinition( attack );
			hitEvent.attackData.AddFlag( hitFlag.CanDamageSelf, 'npc Passenger in vehicle in collision' );
			hitEvent.attackData.AddFlag( hitFlag.NPCPassengerVehicleCollision, 'npc Passenger in vehicle in collision' );
			hitEvent.attackData.AddFlag( hitFlag.ForceNoCrit, 'npc Passenger in vehicle in collision' );
			hitEvent.attackData.SetSource( instigator );
			hitEvent.attackData.SetInstigator( instigator );
			hitEvent.attackData.SetAttackDefinition( attack );
			hitEvent.hitPosition = passenger.GetWorldPosition();
			hitEvent.hitDirection = hitDirection;
			GameInstance.GetDamageSystem( GetVehicle().GetGame() ).QueueHitEvent( hitEvent, GetVehicle() );
			if( VehicleComponent.GetDriver( GetVehicle().GetGame(), GetVehicle(), GetVehicle().GetEntityID() ) == this )
			{
				GetVehicle().ActivateTemporaryLossOfControl();
			}
			passenger.QueueEvent( new VehicleRammedEvent );
		}
	}

