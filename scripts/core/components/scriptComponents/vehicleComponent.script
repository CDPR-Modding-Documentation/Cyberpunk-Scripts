	private function QueueLethalVehicleImpactToAllNonFriendlyAggressivePassengers( sourceName : CName ) : Bool
	{
		var i : Int32;
		var passenger : weak< ScriptedPuppet >;
		var passengers : array< weak< GameObject > >;
		var gi : GameInstance;
		var vehicleID : EntityID;
		var success : Bool;
		gi = GetVehicle().GetGame();
		vehicleID = GetVehicle().GetEntityID();
		success = false;
		GetAllPassengers( gi, vehicleID, false, passengers );
		for( i = 0; i < passengers.Size(); i += 1 )
		{
			passenger = ( ( ScriptedPuppet )( passengers[ i ] ) );
			if( ( passenger && !( IsFriendlyTowardsPlayer( passenger ) ) ) && passenger.IsAggressive() )
			{
				QueueVehicleImpactLethalHitEvent( passenger, passenger, sourceName );
				success = true;
			}
		}
		return success;
	}

