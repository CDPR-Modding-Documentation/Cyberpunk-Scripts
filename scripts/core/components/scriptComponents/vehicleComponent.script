	{
		var vehicle : weak< VehicleObject >;
		var stealEvent : StealVehicleEvent;
		var vehicleHijackEvent : VehicleHijackEvent;
		vehicle = GetVehicle();
		if( !( vehicle ) )
		{
			return;
		}
		if( IsNameValid( slotID.id ) )
		{
			vehicleHijackEvent = new VehicleHijackEvent;
			vehicleHijackEvent.driverAllowedToGetAggressive = slotID.id == GetDriverSlotName();
			QueueEventToPassenger( vehicle.GetGame(), vehicle, slotID, vehicleHijackEvent );
		}
		stealEvent = new StealVehicleEvent;
		vehicle.QueueEvent( stealEvent );
	}

	protected function ToggleVehicleSystems( toggle : Bool, vehicle : Bool, engine : Bool, optional lockState : VehicleQuestEngineLockState )
	{
		if( GetVehicle().IsVehicleOnStateLocked() )
		{
			if( lockState == VehicleQuestEngineLockState.DontToggleIfLocked )
			{
				return;
			}
			GetVehicle().LockVehicleOnState( false );
		}
		if( vehicle )
		{
			GetVehicle().TurnVehicleOn( toggle );
		}
		if( engine )
		{
			GetVehicle().TurnEngineOn( toggle );
		}
		if( lockState == VehicleQuestEngineLockState.Lock )
		{
			GetVehicle().LockVehicleOnState( true );
		}
	}

