	public static function CanBeDriven( gi : GameInstance, vehicle : weak< VehicleObject > ) : Bool
	{
		if( ( ( ( ( ( !( vehicle ) || vehicle.IsDestroyed() ) || ( vehicle.GetAIComponent().GetReservedSeatsCount() > 0 ) ) || HasActiveAutopilot( gi, vehicle ) ) || IsExecutingAnyCommand( gi, vehicle ) ) || vehicle.IsPerformingSceneAnimation() ) || !( vehicle.CommandsFromDriverEnabled() ) )
