	public static function GetDriver( gi : GameInstance, vehicle : weak< VehicleObject >, vehicleID : EntityID ) : weak< GameObject >
	{
		var playerSystem : PlayerSystem;
		var playerObj : weak< GameObject >;
		if( vehicle.IsVehicleRemoteControlled() )
		{
			playerSystem = GameInstance.GetPlayerSystem( gi );
			playerObj = playerSystem.GetLocalPlayerMainGameObject();
			if( playerObj )
			{
				return playerObj;
			}
			else
			{
				return NULL;
			}
		}
		return GetDriverMounted( gi, vehicleID );
	}

