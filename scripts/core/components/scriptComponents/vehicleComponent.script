	public static function QueueEventToAllPassengersRandomly( gi : GameInstance, id : EntityID, evt : Event, min : Float, max : Float ) : Bool
	{
		var i : Int32;
		var passenger : weak< GameObject >;
		var passengers : array< weak< GameObject > >;
		var delayTime : Float;
		if( !( GameInstance.IsValid( gi ) ) || !( EntityID.IsDefined( id ) ) )
		{
			return false;
		}
		GetAllPassengers( gi, id, false, passengers );
		for( i = 0; i < passengers.Size(); i += 1 )
		{
			passenger = passengers[ i ];
			if( passenger )
			{
				delayTime = delayTime + RandRangeF( min, max );
				GameInstance.GetDelaySystem( gi ).DelayEvent( passenger, evt, delayTime );
			}
		}
		if( !( passenger ) )
		{
			return false;
		}
		return true;
	}

	public static function QueueEventToPassenger( gi : GameInstance, vehicleID : EntityID, slotID : MountingSlotId, evt : Event, optional delay : Float, optional randomDelay : Bool ) : Bool
	{
		var i : Int32;
		var passenger : weak< GameObject >;
		var passengers : array< weak< GameObject > >;
		var delayTime : Float;
		if( !( GameInstance.IsValid( gi ) ) || !( EntityID.IsDefined( vehicleID ) ) )
		{
			return false;
		}
		GetAllPassengers( gi, vehicleID, false, passengers );
		for( i = 0; i < passengers.Size(); i += 1 )
		{
			passenger = passengers[ i ];
			if( passenger )
			{
				if( delay > 0.0 )
				{
					GameInstance.GetDelaySystem( gi ).DelayEvent( passenger, evt, delay );
				}
				else if( randomDelay )
				{
					delayTime = delayTime + RandRangeF( 0.1, 2.0 );
					GameInstance.GetDelaySystem( gi ).DelayEvent( passenger, evt, delayTime );
				}
				else
				{
					passenger.QueueEvent( evt );
				}
			}
		}
		if( !( passenger ) )
		{
			return false;
		}
		return true;
	}

	public static function QueueEventToPassenger( gi : GameInstance, vehicle : weak< VehicleObject >, slotID : MountingSlotId, evt : Event, optional delay : Float, optional randomDelay : Bool ) : Bool
	{
		if( !( vehicle ) )
		{
			return false;
		}
		return QueueEventToPassenger( gi, vehicle.GetEntityID(), slotID, evt, delay, randomDelay );
	}

	public static function HasPassengersWithThreatOnPlayer( gi : GameInstance, vehicleID : EntityID ) : Bool
	{
		var passengers : array< weak< GameObject > >;
		var passengerThreats : array< TrackedLocation >;
		var player : weak< GameObject >;
		var i : Int32;
		GetAllPassengers( gi, vehicleID, false, passengers );
		for( i = 0; i < passengers.Size(); i += 1 )
		{
			passengerThreats = ( ( ScriptedPuppet )( passengers[ i ] ) ).GetTargetTrackerComponent().GetThreats( false );
			if( ( passengerThreats.Size() > 0 ) && TargetTrackingExtension.GetPlayerFromThreats( passengerThreats, player ) )
			{
				return true;
			}
		}
		return false;
	}

	public static function GetAllPassengers( gi : GameInstance, vehicleID : EntityID, includeTrunkBody : Bool, passengers : ref< array< weak< GameObject > > > )
	{
		var mountInfos : array< MountingInfo >;
		var i : Int32;
		var passenger : weak< GameObject >;
		if( !( GameInstance.IsValid( gi ) ) || !( EntityID.IsDefined( vehicleID ) ) )
		{
			return;
		}
		mountInfos = GameInstance.GetMountingFacility( gi ).GetMountingInfoMultipleWithIds( , vehicleID );
		for( i = 0; i < mountInfos.Size(); i += 1 )
		{
			if( !( includeTrunkBody ) )
			{
				if( mountInfos[ i ].slotId.id == 'trunk_body' )
				{
					continue;
				}
			}
			passenger = ( ( GameObject )( GameInstance.FindEntityByID( gi, mountInfos[ i ].childId ) ) );
			if( passenger )
			{
				passengers.PushBack( passenger );
			}
		}
		return;
	}

	public static function QueueEventToAllPassengers( gi : GameInstance, vehicleID : EntityID, evt : Event, optional delay : Float, optional randomDelay : Bool ) : Bool
	{
		var i : Int32;
		var passenger : weak< GameObject >;
