	public static function IsMountedToVehicleWithDriverSeatOccupiedByDeadNPC( gi : GameInstance, passengerID : EntityID ) : Bool
	{
		var mountInfos : array< MountingInfo >;
		var driver : ScriptedPuppet;
		var i : Int32;
		if( !( GameInstance.IsValid( gi ) ) || !( EntityID.IsDefined( passengerID ) ) )
		{
			return false;
		}
		mountInfos = GameInstance.GetMountingFacility( gi ).GetMountingInfoMultipleWithIds( passengerID );
		for( i = 0; i < mountInfos.Size(); i += 1 )
		{
			if( IsDriverSlot( mountInfos[ i ].slotId.id ) )
			{
				driver = ( ( ScriptedPuppet )( GameInstance.FindEntityByID( gi, mountInfos[ i ].childId ) ) );
				return driver && !( driver.IsActive() );
			}
		}
		return false;
	}

