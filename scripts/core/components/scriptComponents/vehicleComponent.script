	protected event OnAction( action : ListenerAction, consumer : ListenerActionConsumer )
	{
		var releaseTime : Float;
		var sirenState : Bool;
		var radioEvent : VehicleRadioEvent;
		var toggleEvent : RadioToggleEvent;
		var cycleEvent : UIVehicleRadioCycleEvent;
		var vehicle : VehicleObject;
		if( GetPS().GetIsDestroyed() )
		{
			return false;
		}
		vehicle = GetVehicle();
		if( !( vehicle ) )
		{
			return false;
		}
		if( ListenerAction.GetName( action ) == 'VehicleInsideWheel' )
		{
			if( !( StatusEffectSystem.ObjectHasStatusEffectWithTag( vehicle, 'VehicleBlockRadioInput' ) ) && IsRadioEnabled( vehicle.GetGame() ) )
			{
				if( ListenerAction.IsButtonJustPressed( action ) )
				{
					m_radioPressTime = EngineTime.ToFloat( GameInstance.GetEngineTime( vehicle.GetGame() ) );
				}
				if( ListenerAction.IsButtonJustReleased( action ) )
				{
					releaseTime = EngineTime.ToFloat( GameInstance.GetEngineTime( vehicle.GetGame() ) );
					if( releaseTime <= ( m_radioPressTime + 0.2 ) )
					{
						if( ShouldCycle() )
						{
							radioEvent = new VehicleRadioEvent;
							radioEvent.toggle = true;
							vehicle.QueueEvent( radioEvent );
							if( vehicle.IsRadioReceiverActive() )
							{
								cycleEvent = new UIVehicleRadioCycleEvent;
								GameInstance.GetUISystem( vehicle.GetGame() ).QueueEvent( cycleEvent );
							}
							if( m_mountedPlayer )
							{
								m_mountedPlayer.QueueEvent( radioEvent );
							}
						}
						else
						{
							toggleEvent = new RadioToggleEvent;
							vehicle.QueueEvent( toggleEvent );
							if( m_mountedPlayer )
							{
								m_mountedPlayer.QueueEvent( toggleEvent );
							}
						}
					}
				}
			}
		}
		if( ( ( ListenerAction.GetName( action ) == 'VehicleHornHold' && ListenerAction.GetType( action ) == gameinputActionType.BUTTON_HOLD_COMPLETE ) || ( ListenerAction.GetName( action ) == 'VehicleHorn' && ListenerAction.IsButtonJustPressed( action ) ) ) && !( m_hornOn ) )
		{
			ToggleVehicleHorn( true );
			vehicle.GetStimBroadcasterComponent().TriggerSingleBroadcast( vehicle, gamedataStimType.VehicleHorn );
			m_hornOn = true;
		}
		if( ( ( ListenerAction.GetName( action ) == 'VehicleHornHold' || ListenerAction.GetName( action ) == 'VehicleHorn' ) && m_hornOn ) && ListenerAction.IsButtonJustReleased( action ) )
		{
			ToggleVehicleHorn( false );
			m_hornOn = false;
		}
		if( m_useAuxiliary && ListenerAction.GetName( action ) == 'VehicleSiren' )
		{
			if( ListenerAction.IsButtonJustPressed( action ) )
			{
				m_sirenPressTime = EngineTime.ToFloat( GameInstance.GetEngineTime( vehicle.GetGame() ) );
			}
			if( ListenerAction.IsButtonJustReleased( action ) && ( ( EngineTime.ToFloat( GameInstance.GetEngineTime( GetVehicle().GetGame() ) ) - m_sirenPressTime ) < 0.25 ) )
			{
				sirenState = GetPS().GetSirenState();
				ToggleSiren( !( sirenState ), !( sirenState ) );
			}
		}
	}

