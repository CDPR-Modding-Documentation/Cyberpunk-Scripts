	protected event OnGridDestruction( evt : VehicleGridDestructionEvent )
	{
		var i : Int32;
		var gridState : Float;
		var desiredChange : Float;
		var owner : weak< VehicleObject >;
		var biggestImpact : Float;
		var broadcaster : StimBroadcasterComponent;
		var bike : BikeObject;
		bike = ( ( BikeObject )( GetVehicle() ) );
		for( i = 0; i < evt.rawChange.Size(); i += 1 )
		{
			gridState = evt.state[ i ];
			desiredChange = evt.desiredChange[ i ];
			SendDestructionDataToGraph( i, gridState );
			DetermineAdditionalEngineFX( i, gridState );
			if( desiredChange > biggestImpact )
			{
				biggestImpact = desiredChange;
			}
		}
		PlayVehicleVisialCustomisationCollisionGlitch();
		if( evt.damageMultiplier > 0.0 )
		{
			CreateHitEventOnSelf( evt.damageMultiplier, evt.impactPoint, evt.otherVehicle, evt.rammedOtherVehicle, evt.otherVehicleRammed );
		}
		if( ( ( bike && bike.IsTiltControlEnabled() ) && ( evt.damageMultiplier >= 0.0 ) ) && !( HasActiveDriver( GetVehicle().GetGame(), GetVehicle(), GetVehicle().GetEntityID() ) ) )
		{
			TryToKnockDownBike();
		}
		owner = GetVehicle();
		broadcaster = owner.GetStimBroadcasterComponent();
		if( broadcaster )
		{
			if( biggestImpact < 0.03 )
			{
				broadcaster.TriggerSingleBroadcast( owner, gamedataStimType.CrowdIllegalAction, 10.0 );
			}
			else
			{
				broadcaster.TriggerSingleBroadcast( owner, gamedataStimType.VehicleHit, 5.0 );
			}
		}
	}

	private function CreateHitEventOnSelf( damageMultiplier : Float, impactPoint : Vector3, otherVehicle : weak< GameObject >, rammedOtherVehicle : Bool, otherVehicleRammed : Bool )
	{
		var evt : gameHitEvent;
		var hasDriver : Bool;
		var otherCarIsPlayers : Bool;
		var attackContext : AttackInitContext;
		var attack : IAttack;
		var otherVehicleObject : weak< VehicleObject >;
		hasDriver = HasActiveDriver( GetVehicle().GetGame(), GetVehicle(), GetVehicle().GetEntityID() );
		otherVehicleObject = ( ( VehicleObject )( otherVehicle ) );
		otherCarIsPlayers = ( ( otherVehicleObject ) ? ( otherVehicleObject.IsPlayerDriver() ) : ( false ) );
		evt = new gameHitEvent;
		evt.attackData = new AttackData;
		if( hasDriver && !( otherVehicleRammed ) )
		{
			attackContext.instigator = GetDriver( GetVehicle().GetGame(), GetVehicle(), GetVehicle().GetEntityID() );
			attackContext.source = GetVehicle();
		}
		else if( ( otherVehicle && ( otherVehicleRammed || !( hasDriver ) ) ) && HasActiveDriver( GetVehicle().GetGame(), otherVehicleObject, otherVehicle.GetEntityID() ) )
		{
			attackContext.instigator = GetDriver( otherVehicleObject.GetGame(), otherVehicleObject, otherVehicleObject.GetEntityID() );
			attackContext.source = otherVehicle;
		}
		else if( otherVehicle )
		{
			attackContext.instigator = otherVehicle;
			attackContext.source = otherVehicle;
		}
		else
		{
			attackContext.instigator = GetVehicle();
			attackContext.source = GetVehicle();
		}
		attackContext.record = TweakDBInterface.GetAttackRecord( T"Attacks.VehicleImpact" );
		attack = IAttack.Create( attackContext );
		evt.target = GetVehicle();
		evt.attackData.SetVehicleImpactForce( damageMultiplier );
		evt.attackData.SetAttackPosition( ( ( Vector4 )( impactPoint ) ) );
		evt.hitPosition = ( ( Vector4 )( impactPoint ) );
		if( otherVehicleRammed )
		{
			evt.attackData.AddFlag( hitFlag.CriticalHit, 'VehicleRamming' );
		}
		if( otherCarIsPlayers && !( otherVehicleRammed ) )
		{
			evt.attackData.AddFlag( hitFlag.VehicleImpactWithPlayer, 'VehicleImpact' );
		}
		evt.attackData.AddFlag( hitFlag.VehicleImpact, 'VehicleImpact' );
		evt.attackData.AddFlag( hitFlag.CanDamageSelf, 'VehicleImpact' );
		evt.attackData.AddFlag( hitFlag.FriendlyFire, 'VehicleImpact' );
		evt.attackData.SetAttackType( attackContext.record.AttackType().Type() );
		evt.attackData.SetSource( attackContext.source );
		evt.attackData.SetInstigator( attackContext.instigator );
		evt.attackData.SetAttackTime( EngineTime.ToFloat( GameInstance.GetTimeSystem( GetVehicle().GetGame() ).GetSimTime() ) );
		evt.attackData.SetAttackDefinition( attack );
		evt.attackData.PreAttack();
		GameInstance.GetDamageSystem( GetVehicle().GetGame() ).QueueHitEvent( evt, GetVehicle() );
	}

	public function HandleBikeCollisionReaction( impactVelocityChange : Float, impactHitNormal : Vector4 )
	{
		var recordID : TweakDBID;
		var vehicleRecord : Vehicle_Record;
		var vehicleDataPackage : weak< VehicleDataPackage_Record >;
		var bike : BikeObject;
		var knockOffForce : Float;
		var driver : GameObject;
		var knockDownModifier : Float;
		var animFeatureData : AnimFeature_KnockOffData;
		var delayedKnockOffEvent : DelayedBikeKnockOffEvent;
		bike = ( ( BikeObject )( GetVehicle() ) );
		if( bike && GameInstance.GetCameraSystem( bike.GetGame() ).IsInCameraFrustum( bike, 2.0, 0.75 ) )
		{
			driver = GetDriverMounted( bike.GetGame(), GetVehicle().GetEntityID() );
			recordID = bike.GetRecordID();
			vehicleRecord = TweakDBInterface.GetVehicleRecord( recordID );
			vehicleDataPackage = vehicleRecord.VehDataPackage();
			knockDownModifier = TweakDBInterface.GetFloat( T"AIGeneralSettings.aiBikeKnockOffModifier", 1.0 );
			knockOffForce = vehicleDataPackage.KnockOffForce() * ( ( ( ( NPCPuppet )( driver ) ) ) ? ( knockDownModifier ) : ( 1.0 ) );
			if( ( impactVelocityChange > knockOffForce ) || IsBeingDragged() )
			{
				if( IsMountedToVehicle( bike.GetGame(), driver ) )
				{
					bike.EnableAirControl( false );
					delayedKnockOffEvent = new DelayedBikeKnockOffEvent;
					GameInstance.GetDelaySystem( bike.GetGame() ).DelayEvent( GetVehicle(), delayedKnockOffEvent, 0.40000001 );
					if( ( ( NPCPuppet )( driver ) ) )
					{
						GameInstance.GetDelaySystem( driver.GetGame() ).DelayEvent( driver, CreateForceRagdollEvent( 'BikeknockDownEvent' ), 0.30000001 );
						m_timeSinceLastHit = 0.0;
						animFeatureData = new AnimFeature_KnockOffData;
						animFeatureData.knockedOff = true;
						animFeatureData.direction = GameObject.GetLocalAngleForDirectionInInt( impactHitNormal, driver );
						animFeatureData.force = impactVelocityChange;
						animFeatureData.draggedOff = IsBeingDragged();
						AnimationControllerComponent.ApplyFeatureToReplicate( driver, 'KnockOffData', animFeatureData );
						AnimationControllerComponent.PushEventToReplicate( driver, 'KnockOffData' );
					}
				}
			}
		}
	}

	protected event OnDelayedBikeKnockOffEvent( evt : DelayedBikeKnockOffEvent )
	{
		var knockOverBike : KnockOverBikeEvent;
		var bike : BikeObject;
		var aiEvent : AIEvent;
		var driver : GameObject;
		var collisionForce : Vector4;
		var recordID : TweakDBID;
		var vehicleRecord : Vehicle_Record;
		var vehicleDataPackage : weak< VehicleDataPackage_Record >;
		bike = ( ( BikeObject )( GetVehicle() ) );
		driver = GetDriverMounted( bike.GetGame(), GetVehicle().GetEntityID() );
		if( driver != NULL )
		{
			if( ( ( NPCPuppet )( driver ) ) )
			{
				GameInstance.GetWorkspotSystem( bike.GetGame() ).UnmountFromVehicle( GetVehicle(), driver, true, , , 'Bumped' );
				knockOverBike = new KnockOverBikeEvent;
				knockOverBike.forceKnockdown = true;
				knockOverBike.applyDirectionalForce = true;
				bike.QueueEvent( knockOverBike );
				aiEvent = new AIEvent;
				aiEvent.name = 'NoDriver';
				bike.QueueEvent( aiEvent );
			}
			else
			{
				recordID = GetVehicle().GetRecordID();
				vehicleRecord = TweakDBInterface.GetVehicleRecord( recordID );
				vehicleDataPackage = vehicleRecord.VehDataPackage();
				collisionForce = -( bike.GetLinearVelocity() );
				if( Vector4.Length( collisionForce ) <= vehicleDataPackage.KnockOffForce() )
				{
					bike.EnableAirControl( true );
					bike.EnableTiltControl( true );
					return false;
				}
				bike.AddCollisionForce( collisionForce );
			}
		}
		ToggleVehicleSystems( false, false, false );
	}

	public function CheckForDrag( impactImpulse : Float )
	{
		var currentTimeStamp : Float;
		var isValidData : Bool;
		var deltaTime : Float;
		currentTimeStamp = EngineTime.ToFloat( GameInstance.GetSimTime( GetVehicle().GetGame() ) );
		deltaTime = currentTimeStamp - m_timeSinceLastHit;
		isValidData = ( m_timeSinceLastHit > 0.0 ) && ( deltaTime > 0.0 );
		if( ( ( m_timeSinceLastHit != 0.0 ) && isValidData ) && ( deltaTime < 1.0 ) )
		{
			m_dragTime += deltaTime;
		}
		else
		{
			m_dragTime = 0.0;
		}
		m_timeSinceLastHit = currentTimeStamp;
	}

	public function IsBeingDragged() : Bool
	{
		return m_dragTime > 1.5;
	}

	private function TryToKnockDownBike()
