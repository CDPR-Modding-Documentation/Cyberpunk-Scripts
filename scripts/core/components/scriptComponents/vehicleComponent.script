	protected event OnMountingEvent( evt : MountingEvent )
	{
		var mountChild : GameObject;
		var PSvehicleDooropenRequest : VehicleDoorOpen;
		var vehicleDataPackage : weak< VehicleDataPackage_Record >;
		var vehicleRecord : Vehicle_Record;
		var gameInstance : GameInstance;
		var isDriverSlot : Bool;
		var shouldAutoClose : Bool;
		shouldAutoClose = true;
		gameInstance = GetVehicle().GetGame();
		mountChild = ( ( GameObject )( GameInstance.FindEntityByID( gameInstance, evt.request.lowLevelMountingInfo.childId ) ) );
		GetVehicleDataPackage( gameInstance, GetVehicle(), vehicleDataPackage );
		if( mountChild.IsPlayer() )
		{
			m_mountedPlayer = ( ( PlayerPuppet )( mountChild ) );
			isDriverSlot = VehicleComponent.IsDriverSlot( evt.request.lowLevelMountingInfo.slotId.id );
			if( isDriverSlot )
			{
				PreventionSystem.SetPlayerMounted( gameInstance, true );
				PreventionSystemHackerLoop.UpdatePlayerVehicle( gameInstance, GetVehicle() );
				PoliceRadioScriptSystem.UpdatePoliceRadioOnVehicleEntrance( gameInstance );
			}
			m_mountedPlayer.GetPlayerStateMachineBlackboard().SetBool( GetAllBlackboardDefs().PlayerStateMachine.MountedToVehicleInDriverSeat, isDriverSlot );
			GetVehicleRecord( gameInstance, m_mountedPlayer, vehicleRecord );
			QueueEventToAllPassengers( m_mountedPlayer.GetGame(), GetVehicle().GetEntityID(), PlayerMuntedToMyVehicle.Create( m_mountedPlayer ) );
			PlayerPuppet.ReevaluateAllBreathingEffects( ( ( PlayerPuppet )( mountChild ) ) );
			if( !( GetVehicle().IsCrowdVehicle() ) )
			{
				GetVehicle().GetDeviceLink().TriggerSecuritySystemNotification( GetVehicle().GetWorldPosition(), mountChild, ESecurityNotificationType.ALARM );
			}
			ToggleScanningComponent( false );
			if( GetVehicle().GetHudManager().IsRegistered( GetVehicle().GetEntityID() ) )
			{
				RegisterToHUDManager( false );
			}
			if( GetVehicle().IsPlayerVehicle() && !( GetPS().GetIsDestroyed() ) )
			{
				CreateMappin();
			}
			RegisterInputListener();
			FastTravelSystem.AddFastTravelLock( 'InVehicle', gameInstance );
			m_mounted = true;
			m_ignoreAutoDoorClose = true;
			SetupListeners();
			DisableTargetingComponents();
			if( EntityID.IsDefined( evt.request.mountData.mountEventOptions.entityID ) )
			{
				m_enterTime = vehicleDataPackage.Stealing() + vehicleDataPackage.SlideDuration();
			}
			else if( ( m_mountedPlayer.IsInCombat() && vehicleRecord.Type().Type() == gamedataVehicleType.Car ) && isDriverSlot )
			{
				m_enterTime = vehicleDataPackage.CombatEntering() + vehicleDataPackage.SlideDuration();
				if( vehicleDataPackage.DriverCombat().Type() == gamedataDriverCombatType.Doors && ( ( ( WeaponObject )( GameInstance.GetTransactionSystem( m_mountedPlayer.GetGame() ).GetItemInSlot( m_mountedPlayer, T"AttachmentSlots.WeaponLeft" ) ) ) || ( ( WeaponObject )( GameInstance.GetTransactionSystem( m_mountedPlayer.GetGame() ).GetItemInSlot( m_mountedPlayer, T"AttachmentSlots.WeaponRight" ) ) ) ) )
				{
					shouldAutoClose = false;
				}
				else
				{
					shouldAutoClose = true;
				}
			}
			else
			{
				m_enterTime = vehicleDataPackage.Entering() + vehicleDataPackage.SlideDuration();
			}
			DrivingStimuli( true );
			if( evt.request.lowLevelMountingInfo.slotId.id == GetDriverSlotName() )
			{
				if( ( ( TankObject )( GetVehicle() ) ) )
				{
					TogglePlayerHitShapesForPanzer( m_mountedPlayer, false );
					ToggleTargetingSystemForPanzer( m_mountedPlayer, true );
				}
				SetSteeringLimitAnimFeature( 1 );
			}
			GameInstance.GetStatPoolsSystem( GetVehicle().GetGame() ).RequestSettingStatPoolValueCustomLimit( GetVehicle().GetEntityID(), gamedataStatPoolType.Health, m_healthDecayThreshold, GetVehicle() );
		}
		if( !( mountChild.IsPlayer() ) )
		{
			if( evt.request.mountData.isInstant )
			{
				mountChild.QueueEvent( CreateDisableRagdollEvent( 'VehicleComponentOnMountingEvent' ) );
			}
			PushVehicleNPCData( gameInstance, mountChild );
			if( ( mountChild.IsPuppet() && !( GetVehicle().IsPlayerVehicle() ) ) && ( IsHostileTowardsPlayer( mountChild ) || ( ( ScriptedPuppet )( mountChild ) ).IsAggressive() ) )
			{
				EnableTargetingComponents();
			}
		}
		if( !( evt.request.mountData.isInstant ) )
		{
			PSvehicleDooropenRequest = new VehicleDoorOpen;
			PSvehicleDooropenRequest.slotID = GetVehicle().GetBoneNameFromSlot( evt.request.lowLevelMountingInfo.slotId.id );
			if( EntityID.IsDefined( evt.request.mountData.mountEventOptions.entityID ) )
			{
				PSvehicleDooropenRequest.autoCloseTime = vehicleDataPackage.Stealing_open();
			}
			else
			{
				PSvehicleDooropenRequest.autoCloseTime = vehicleDataPackage.Normal_open();
			}
			if( !( GetPS().GetIsDestroyed() ) && shouldAutoClose )
			{
				PSvehicleDooropenRequest.shouldAutoClose = true;
			}
			GameInstance.GetPersistencySystem( gameInstance ).QueuePSEvent( GetPS().GetID(), GetPS().GetClassName(), PSvehicleDooropenRequest );
		}
		ManageAdditionalAnimFeatures( mountChild, true );
		if( mountChild.IsPrevention() )
		{
			m_preventionPassengers += 1;
			RegisterWantedLevelListener();
			GameInstance.GetPreventionSpawnSystem( gameInstance ).RegisterEntityDeathCallback( this, "OnPreventionPassengerDeath", mountChild.GetEntityID() );
			CreateMappin();
			GetVehicle().GetPreventionSystem().UpdateVehiclePassengerCount( GetVehicle().GetEntityID(), m_preventionPassengers );
		}
	}

	protected event OnUnmountingEvent( evt : UnmountingEvent )
	{
		var mountChild : GameObject;
		var activePassengers : Int32;
		var mountChildIsPrevention : Bool;
		var gameInstance : GameInstance;
		gameInstance = GetVehicle().GetGame();
		mountChild = ( ( GameObject )( GameInstance.FindEntityByID( gameInstance, evt.request.lowLevelMountingInfo.childId ) ) );
		mountChildIsPrevention = mountChild && mountChild.IsPrevention();
		VehicleComponent.SetAnimsetOverrideForPassenger( mountChild, evt.request.lowLevelMountingInfo.parentId, evt.request.lowLevelMountingInfo.slotId.id, 0.0 );
		if( mountChild && mountChild.IsPlayer() )
		{
			PreventionSystem.SetPlayerMounted( gameInstance, false );
			PlayerPuppet.ReevaluateAllBreathingEffects( ( ( PlayerPuppet )( mountChild ) ) );
			ToggleScanningComponent( true );
			if( GetVehicle().ShouldRegisterToHUD() )
			{
				RegisterToHUDManager( true );
			}
			UnregisterInputListener();
			FastTravelSystem.RemoveFastTravelLock( 'InVehicle', gameInstance );
			m_mounted = false;
			UnregisterListeners();
			ToggleSiren( false, false );
			if( m_broadcasting )
			{
				DrivingStimuli( false );
			}
			if( evt.request.lowLevelMountingInfo.slotId.id == 'seat_front_left' )
			{
				PreventionSystemHackerLoop.UpdatePlayerVehicle( gameInstance, NULL );
				ToggleCrystalDome( false, true, false, 0.0, 0.0, true );
				SetSteeringLimitAnimFeature( 0 );
				m_ignoreAutoDoorClose = false;
			}
			DoPanzerCleanup();
			m_mountedPlayer = NULL;
			CleanUpRace();
			GameInstance.GetStatPoolsSystem( GetVehicle().GetGame() ).RequestSettingStatPoolValueCustomLimit( GetVehicle().GetEntityID(), gamedataStatPoolType.Health, 0.0, GetVehicle() );
			GameInstance.GetAudioSystem( GetVehicle().GetGame() ).RemoveTriggerEffect( 'te_vehicle_car_disabled' );
		}
		if( mountChild )
		{
			if( mountChildIsPrevention )
			{
				GameInstance.GetPreventionSpawnSystem( gameInstance ).UnregisterEntityDeathCallback( this, "OnPreventionPassengerDeath", mountChild.GetEntityID() );
				if( mountChild.IsActive() )
				{
					m_preventionPassengers = Max( 0, m_preventionPassengers - 1 );
				}
				GetVehicle().GetPreventionSystem().UpdateVehiclePassengerCount( GetVehicle().GetEntityID(), m_preventionPassengers );
				if( m_preventionPassengers == 0 )
				{
					DestroyMappin();
				}
			}
			if( GetNumberOfActivePassengers( mountChild.GetGame(), GetVehicle().GetEntityID(), activePassengers ) )
			{
				if( activePassengers <= 0 )
				{
					DisableTargetingComponents();
				}
			}
		}
		GetPS().ToggleReserveSeatDuringUnmounting( false, evt.request.lowLevelMountingInfo.slotId.id );
		ManageAdditionalAnimFeatures( mountChild, false );
	}

