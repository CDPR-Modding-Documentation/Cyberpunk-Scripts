	protected function ApplyVehicleVisualCustomizationTemplate( template : VehicleVisualCustomizationTemplate, isInstant : Bool )
	{
		var packedColorA, packedColorB : Float;
		var record : Vehicle_Record;
		record = TweakDBInterface.GetVehicleRecord( GetVehicle().GetRecordID() );
		m_customizationComponent.SetDefaultDecalsEnabled( false );
		if( VehicleVisualCustomizationTemplate.GetType( template ) == VehicleVisualCustomizationType.Generic )
		{
			packedColorA = PackColorForEffect( GenericTemplatePersistentData.GetPrimaryColor( template.genericData ) );
			packedColorB = PackColorForEffect( GenericTemplatePersistentData.GetSecondaryColor( template.genericData ) );
			effectBlackboard = new worldEffectBlackboard;
			effectBlackboard.SetValueUnclamped( 'colorA', packedColorA );
			effectBlackboard.SetValueUnclamped( 'colorB', packedColorB );
			m_customizationComponent.SetupGenericCustomization( record.ExcludedComponentsGeneric(), record.GenericCustomizationMask() );
			PlayVehicleVisualCustomizationShader( true, isInstant, effectBlackboard, record.CustomizeCarRims() );
		}
		else
		{
			m_customizationComponent.SetupUniqueCustomization( template.uniqueData.customMultilayers, template.uniqueData.globalClearCoatOverrides, template.uniqueData.partsClearCoatOverrides );
			m_customizationComponent.ShowUniqueCustomizationDecalsWithDisolve( template.uniqueData.customDecals );
			PlayVehicleVisualCustomizationShader( true, isInstant, NULL, false );
		}
	}

	public function PackColorForEffect( color : Color ) : Float
	{
		var packedValue : Float;
		var rNormalized : Float;
		var gNormalized : Float;
		var bNormalized : Float;
		rNormalized = 1.0 - RoundTo( color.Red / 255.0, 2 );
		gNormalized = 1.0 - RoundTo( color.Green / 255.0, 2 );
		bNormalized = 1.0 - RoundTo( color.Blue / 255.0, 2 );
		rNormalized = ClampF( rNormalized, 0.01, 0.99000001 );
		gNormalized = ClampF( gNormalized, 0.01, 0.99000001 );
		bNormalized = ClampF( bNormalized, 0.01, 0.99000001 );
		packedValue = ( rNormalized + ( gNormalized / 100.0 ) ) + ( bNormalized / 10000.0 );
		return packedValue;
	}

	public function ProcessHeatLevelOnVisualCustomization( previousTemplate : VehicleVisualCustomizationTemplate, newTemplate : VehicleVisualCustomizationTemplate )
	{
		var preventionSystem : PreventionSystem;
		var isNewProfileDifferentEnough : Bool;
		preventionSystem = ( ( PreventionSystem )( GameInstance.GetScriptableSystemsContainer( GetVehicle().GetGame() ).Get( 'PreventionSystem' ) ) );
		if( !( preventionSystem.IsChasingPlayer() ) )
		{
			return;
		}
		if( ( preventionSystem.GetHeatStageAsInt() > 4 ) || preventionSystem.GetStarState() == EStarState.Active )
		{
			return;
		}
		if( VehicleVisualCustomizationTemplate.Equals( previousTemplate, newTemplate ) || ( !( VehicleVisualCustomizationTemplate.IsValid( previousTemplate ) ) && !( VehicleVisualCustomizationTemplate.IsValid( newTemplate ) ) ) )
		{
			return;
		}
		if( VehicleVisualCustomizationTemplate.GetType( previousTemplate ) != VehicleVisualCustomizationTemplate.GetType( newTemplate ) )
		{
			isNewProfileDifferentEnough = true;
		}
		else if( !( VehicleVisualCustomizationTemplate.IsValid( previousTemplate ) ) ^ !( VehicleVisualCustomizationTemplate.IsValid( newTemplate ) ) )
		{
			isNewProfileDifferentEnough = true;
		}
		else if( VehicleVisualCustomizationTemplate.GetType( previousTemplate ) == VehicleVisualCustomizationType.Generic )
		{
			isNewProfileDifferentEnough = IsColorChangeLargeEnoughForPolice( GenericTemplatePersistentData.GetPrimaryColor( previousTemplate.genericData ), GenericTemplatePersistentData.GetPrimaryColor( newTemplate.genericData ) ) || IsColorChangeLargeEnoughForPolice( GenericTemplatePersistentData.GetSecondaryColor( previousTemplate.genericData ), GenericTemplatePersistentData.GetSecondaryColor( newTemplate.genericData ) );
		}
		else if( VehicleVisualCustomizationTemplate.GetType( previousTemplate ) == VehicleVisualCustomizationType.Unique )
		{
			isNewProfileDifferentEnough = true;
		}
		if( !( isNewProfileDifferentEnough ) )
		{
			return;
		}
		preventionSystem.QueueRequest( new PreventionSearchingStatusRequest );
		SendPoliceNotification();
	}

	private function IsColorChangeLargeEnoughForPolice( colorA : Color, colorB : Color ) : Bool
	{
		var squaredDifference : Int32;
		squaredDifference = ( ( ( Int32 )( colorB.Red ) ) - ( ( Int32 )( colorA.Red ) ) ) * ( ( ( Int32 )( colorB.Red ) ) - ( ( Int32 )( colorA.Red ) ) );
		squaredDifference += ( ( ( ( Int32 )( colorB.Green ) ) - ( ( Int32 )( colorA.Green ) ) ) * ( ( ( Int32 )( colorB.Green ) ) - ( ( Int32 )( colorA.Green ) ) ) );
		squaredDifference += ( ( ( ( Int32 )( colorB.Blue ) ) - ( ( Int32 )( colorA.Blue ) ) ) * ( ( ( Int32 )( colorB.Blue ) ) - ( ( Int32 )( colorA.Blue ) ) ) );
		return squaredDifference >= ( ( Int32 )( MathHelper.SqrF( ( 255.0 * 3.0 ) / 10.0 ) ) );
	}

