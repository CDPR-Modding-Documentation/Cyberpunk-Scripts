			}
			if( m_mounted )
			{
				DrivingStimuli( true );
			}
		}
		if( evt.newState != vehicleEState.On )
		{
			if( !( crystalDomeQuestModified ) )
			{
				if( GetVehicle() != ( ( AVObject )( GetVehicle() ) ) )
				{
					ToggleCrystalDome( false );
				}
			}
			DrivingStimuli( false );
		}
		if( !( GetPS().GetIsDefaultLightToggleSet() ) )
		{
			recordID = GetVehicle().GetRecordID();
			record = TweakDBInterface.GetVehicleRecord( recordID );
			defaultState = record.VehDefaultState();
			size = defaultState.GetLightToggleComponentsCount();
			for( i = 0; i < size; i += 1 )
			{
				evnt = new VehicleLightQuestToggleEvent;
				evnt.lightType = ( ( vehicleELightType )( defaultState.GetLightToggleComponentsItem( i ) ) );
				evnt.toggle = defaultState.DefaultLightToggle();
				GetVehicle().QueueEvent( evnt );
			}
			GetPS().SetIsDefaultLightToggleSet( true );
		}
	}

