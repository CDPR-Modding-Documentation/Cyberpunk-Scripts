			break;
			case gamedataQuality.Rare:
				canCraft = playerLevel >= 1.0;
			break;
			case gamedataQuality.Epic:
				canCraft = playerLevel >= 1.0;
			break;
			case gamedataQuality.Legendary:
				canCraft = playerLevel >= 1.0;
			break;
			default:
				canCraft = true;
			break;
