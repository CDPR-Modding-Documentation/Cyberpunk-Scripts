		var requiredIngredients : array< IngredientData >;
		if( RPGManager.IsItemMaxTier( itemData ) )
		{
			return false;
		}
		requiredIngredients = GetItemFinalUpgradeCost( itemData );
		result = HasIngredients( requiredIngredients );
