function IntToEPreventionHeatStage( index : Int32 ) : EPreventionHeatStage
{
	if( index < 0 )
	{
		index = 0;
	}
	if( index >= ( ( Int32 )( EPreventionHeatStage.Size ) ) )
	{
		index = ( ( Int32 )( EPreventionHeatStage.Size ) ) - 1;
	}
	return ( ( EPreventionHeatStage )( index ) );
}

